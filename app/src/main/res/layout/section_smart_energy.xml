<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tvTitle5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        android:text="스마트 절전"
        android:textSize="16dp"
        android:fontFamily="@font/noto_sans_kr_regular"
        android:includeFontPadding="false"
        android:textColor="@android:color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btnTelevision"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="20dp"
        android:paddingTop="22dp"
        android:paddingLeft="12dp"
        android:paddingBottom="22dp"
        android:background="@drawable/rec_12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/btnAirConditioner"
        app:layout_constraintTop_toBottomOf="@id/tvTitle5">
        <ImageView
            android:id="@+id/iv1"
            android:layout_width="28dp"
            android:layout_height="28dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/television"
            app:tint="@android:color/black" />
        <TextView
            android:id="@+id/energyType1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="거실 티비"
            android:textSize="15dp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            android:textColor="@android:color/black"
            app:layout_constraintTop_toTopOf="@id/iv1"
            app:layout_constraintStart_toEndOf="@id/iv1"
            app:layout_constraintBottom_toTopOf="@id/energyStatus1"
            app:layout_constraintVertical_chainStyle="packed"/>
        <TextView
            android:id="@+id/energyStatus1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사용안함"
            android:textSize="12dp"
            android:textColor="#AAAAAA"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/energyType1"
            app:layout_constraintStart_toStartOf="@id/energyType1"
            app:layout_constraintBottom_toBottomOf="@id/iv1"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btnAirConditioner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rec_12_border_gradient"
        android:layout_marginRight="20dp"
        android:layout_marginLeft="12dp"
        android:paddingTop="22dp"
        android:paddingLeft="12dp"
        android:paddingBottom="22dp"
        app:layout_constraintStart_toEndOf="@id/btnTelevision"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/btnTelevision">
        <ImageView
            android:id="@+id/iv2"
            android:layout_width="27dp"
            android:layout_height="27dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/wind"
            app:tint="@android:color/black" />
        <TextView
            android:id="@+id/energyType2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="13dp"
            android:text="거실 에어컨"
            android:textSize="15dp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            android:textColor="@android:color/black"
            app:layout_constraintTop_toTopOf="@id/iv2"
            app:layout_constraintStart_toEndOf="@id/iv2"
            app:layout_constraintBottom_toTopOf="@id/energyStatus2"
            app:layout_constraintVertical_chainStyle="packed"/>
        <TextView
            android:id="@+id/energyStatus2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사용안함"
            android:textSize="12dp"
            android:textColor="#AAAAAA"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/energyType2"
            app:layout_constraintStart_toStartOf="@id/energyType2"
            app:layout_constraintBottom_toBottomOf="@id/iv2"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btnLight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rec_12_border_gradient"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="20dp"
        android:paddingTop="22dp"
        android:paddingLeft="12dp"
        android:paddingBottom="22dp"
        app:layout_constraintTop_toBottomOf="@id/btnTelevision"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/btnMicrowave">
        <ImageView
            android:id="@+id/iv3"
            android:layout_width="27dp"
            android:layout_height="27dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:src="@drawable/lamp"
            app:tint="@android:color/black" />
        <TextView
            android:id="@+id/energyType3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="거실 전등"
            android:textSize="15dp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            android:textColor="@android:color/black"
            app:layout_constraintTop_toTopOf="@id/iv3"
            app:layout_constraintStart_toEndOf="@id/iv3"
            app:layout_constraintBottom_toTopOf="@id/energyStatus3"
            app:layout_constraintVertical_chainStyle="packed"/>
        <TextView
            android:id="@+id/energyStatus3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사용안함"
            android:textSize="12dp"
            android:textColor="#AAAAAA"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/energyType3"
            app:layout_constraintStart_toStartOf="@id/energyType3"
            app:layout_constraintBottom_toBottomOf="@id/iv3"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btnMicrowave"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/rec_12_border_gradient"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="20dp"
        android:paddingTop="22dp"
        android:paddingLeft="12dp"
        android:paddingBottom="22dp"
        app:layout_constraintTop_toTopOf="@id/btnLight"
        app:layout_constraintStart_toEndOf="@id/btnLight"
        app:layout_constraintEnd_toEndOf="parent">
        <ImageView
            android:id="@+id/iv4"
            android:layout_width="32dp"
            android:layout_height="32dp"
            android:src="@drawable/microwave"
            app:tint="@android:color/black"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
        <TextView
            android:id="@+id/energyType4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="가스레인지"
            android:textSize="15dp"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            android:textColor="@android:color/black"
            app:layout_constraintTop_toTopOf="@id/iv4"
            app:layout_constraintStart_toEndOf="@id/iv4"
            app:layout_constraintBottom_toTopOf="@id/energyStatus4"
            app:layout_constraintVertical_chainStyle="packed"/>
        <TextView
            android:id="@+id/energyStatus4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="사용안함"
            android:textSize="12dp"
            android:textColor="#AAAAAA"
            android:fontFamily="@font/noto_sans_kr_regular"
            android:includeFontPadding="false"
            app:layout_constraintTop_toBottomOf="@id/energyType4"
            app:layout_constraintStart_toStartOf="@id/energyType4"
            app:layout_constraintBottom_toBottomOf="@id/iv4"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>